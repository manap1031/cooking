<div class="background-search">
  <div class="main-search">
    <div class="fancybox20">
      <h1 class="title-search">検索結果</h1>
      <div class="log-search">
      <%= link_to "もどる", root_path %>
     </div>
    </div>
  </div>

  <%= form_with(url: search_cooks_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
  <% end %>


  <div class="item-contents-search">
    <ul class="item-list-search">
      <div class="contents-row-search">
         <% @cooks.each do |cook| %>
         <li>
         </li>
        <% end %>
      </div>
    </ul>
  </div>
</div>